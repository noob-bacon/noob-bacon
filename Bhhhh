local OrionLib = loadstring(game:HttpGet(('https://raw.githubusercontent.com/shlexware/Orion/main/source')))()
local Window = OrionLib:MakeWindow({Name = "Leaked", HidePremium = false, SaveConfig = true, ConfigFolder = "OrionTest"})

local Tab = Window:MakeTab({
	Name = "Trainee",
	Icon = "rbxassetid://4483345998",
	PremiumOnly = false
})

Tab:AddLabel("Notes Stage")

Tab:AddButton({
	Name = "Welcome to the training session!",
	Callback = function()
      		        if setclipboard then
            setclipboard("Welcome to the training session! As your trainer, I will guide you through everything you need to do in order to rank up.") -- Set clipboard text
            print("Clipboard updated!")
        else
            warn("setclipboard function is not supported in this environment.")
        end
  	end    
})

--[[
Name = <string> - The name of the button.
Callback = <function> - The function of the button.
]]
