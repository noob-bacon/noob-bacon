
local OrionLib = loadstring(game:HttpGet(('https://raw.githubusercontent.com/shlexware/Orion/main/source')))()

local Window = OrionLib:MakeWindow({Name = "tds", HidePremium = false, SaveConfig = true, ConfigFolder = "OrionTest"})

local Tab = Window:MakeTab({
	Name = "Autos",
	Icon = "rbxassetid://4483345998",
	PremiumOnly = false
})

Tab:AddButton({
	Name = "Auto Commander",
	Callback = function()
      		local balls = workspace.Towers:GetChildren()
local ballscounter = 1

for _, tower in pairs(balls) do
    if tower.Name == "Galactic" then
        tower.Name = "Galactic" .. ballscounter
        ballscounter += 1 
    end
end

while true do
    for i = 1, ballscounter - 1 do
        local args = {
            [1] = "Troops",
            [2] = "Abilities",
            [3] = "Activate",
            [4] = {
                ["Troop"] = workspace.Towers["Galactic" .. i],
                ["Name"] = "Call Of Arms",
                ["Data"] = {}
            }
        }

        game:GetService("ReplicatedStorage").RemoteFunction:InvokeServer(unpack(args))
        wait(10)
    end
end

  	end    
})

--[[
Name = <string> - The name of the button.
Callback = <function> - The function of the button.
]]

Tab:AddButton({
	Name = "Auto Medic",
	Callback = function()


      		loadstring(game:HttpGet(('https://raw.githubusercontent.com/TacoCatBackWardsIsTacoCat/scripts/main/TDS%20Auto%20Medic.lua')))()
  	end    
})

--[[
Name = <string> - The name of the button.
Callback = <function> - The function of the button.
]]
