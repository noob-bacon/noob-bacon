https://linkvertise.com/1054764/anime-last-stand-buanghub?o=sharing Window:AddTab({ Title = "Teleport", Icon = "map-pin" }),
    Misc = Window:AddTab({ Title = "Misc", Icon = "settings" })
}

-- Lobby Tab
Tabs.Lobby:AddParagraph({
    Title = "Free Roles",
    Content = "Select a role"
})

Tabs.Lobby:AddButton({
    Title = "Police",
    Description = "Get Gun",
    Callback = function()
        local Event = game:GetService("ReplicatedStorage").RemoteEvents.OutsideRole
        Event:FireServer("Gun", true)
    end
})

Tabs.Lobby:AddButton({
    Title = "Swat",
    Description = "Get SwatGun",
    Callback = function()
        local Event = game:GetService("ReplicatedStorage").RemoteEvents.OutsideRole
        Event:FireServer("SwatGun", true)
    end
})

