-- Teleport Tab
Tabs.Teleport:AddButton({
    Title = "House",
    Callback = function()
        game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(-36, 3, -200)
    end
})

Tabs.Teleport:AddButton({
    Title = "Basement",
    Callback = function()
        game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(71, -15, -163)
    end
})

Tabs.Teleport:AddButton({
    Title = "Attic",
    Callback = function()
        game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(-16, 35, -220)
    end
})

Tabs.Teleport:AddButton({
    Title = "Upstairs",
    Callback = function()
        game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(-15.68, 18.25, -218.67)
    end
})

Tabs.Teleport:AddButton({
    Title = "Hiding Spot",
    Callback = function()
        game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(-42.87, 6.43, -222.01)
    end
})

