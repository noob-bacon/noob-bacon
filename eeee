while wait() do
local orbs = game:GetService("Workspace").GameAssets.GlobalAssets.OrbSpawns
for i, v in pairs(orbs:GetChildren()) do
    if v:IsA("MeshPart") then 
        local user = game.Players.LocalPlayer.Character.Head
        firetouchinterest(user, v, 0) -- Fire touch interest between user and orb (MeshPart)
    elseif v:IsA("Model") then
        local summerOrb = v:FindFirstChild("Orb.1")
        if summerOrb then
            firetouchinterest(game.Players.LocalPlayer.Character.Head, summerOrb, 0) -- Fire touch interest between user and the Orb inside the Model
        end
    end
end
end
